@model SettingsModel

<h2>Settings</h2>
<p>File Name: @Model.FileName</p>
@{
    string[] labels = new string[] { "employedBees", "onlookerBees", "limit" };
}
<form asp-action="Index" method="post">
    @for (var i = 0; i < Model.Values.Count; i++)
    {
        <div>
            <label for="Values[@i]">@labels[i] @(i + 1):</label>
            <input asp-for="Values[i]" class="form-control" />
        </div>
    }

    <button type="submit" class="btn btn-primary">Save</button>
</form>
